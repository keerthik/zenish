{
    "git.enabled": false,
    "explorer.autoReveal": false,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.openEditors.visible": 0,
    "editor.suggest.showReferences": false,
    "editor.codeLens": true,
    "editor.fontSize": 15,
    "editor.lineNumbers": "on",
    "editor.matchBrackets": "near",
    "editor.autoClosingBrackets": "never",
    "editor.minimap.enabled": false,
    "editor.codeActionsOnSave": {
        "source.organizeImports": "explicit"
    },
    "editor.gotoLocation.multipleTypeDefinitions": "gotoAndPeek",
    "settings.cycle": [{
        "id": "codeLens",
        "overrideWorkspaceSettings": true,
        "values": [{
                "editor.codeLens": true,
                "editor.guides.highlightActiveBracketPair": true,
                "editor.guides.bracketPairs": true,
                "editor.lineNumbers": "on",
                "window.commandCenter": true,
                "workbench.statusBar.visible": true,
                "workbench.layoutControl.enabled": true,
                "workbench.colorCustomizations": {
                    "editorError.foreground": "default",
                    "editorWarning.foreground": "default",
                    "editorOverviewRuler.errorForeground": "default",
                },
            },
            {
                "editor.codeLens": false,
                "editor.guides.highlightActiveBracketPair": false,
                "editor.guides.bracketPairs": false,
                "editor.lineNumbers": "off",
                "window.commandCenter": false,
                "workbench.statusBar.visible": false,
                "workbench.layoutControl.enabled": false,
                "workbench.colorCustomizations": {
                    "editorError.foreground": "#573243",
                    "editorWarning.foreground": "#224033",
                    "editorOverviewRuler.errorForeground": "#573243",
                },
            }]
    }],
    "workbench.iconTheme": null,
    "workbench.colorTheme": "eppz!",
    "workbench.editor.enablePreview": false,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "workbench.editor.revealIfOpen": true,
    "omnisharp.useGlobalMono": "always",
    "omnisharp.useModernNet": true,
    "zenMode.hideStatusBar": true,
    "editor.gotoLocation.multipleReferences": "goto",
    "editor.gotoLocation.multipleImplementations": "goto",
    "editor.gotoLocation.multipleDefinitions": "goto",
    "editor.gotoLocation.multipleDeclarations": "goto",
    "workbench.statusBar.visible": true,
    "workbench.editor.decorations.badges": false,
    "workbench.editor.decorations.colors": false,
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter.notebook.ipynb"
    },
    "security.workspace.trust.untrustedFiles": "open",
    "workbench.enableExperiments": false,
    "update.showReleaseNotes": false,
    "telemetry.telemetryLevel": "off",
    "editor.inlineSuggest.enabled": true,
    "workbench.colorCustomizations": {
        "editorError.foreground": "default",
        "editorWarning.foreground": "default",
        "editorOverviewRuler.errorForeground": "default"
    },
    "eppz-code.disableAnalytics": true,
    "editor.guides.highlightActiveBracketPair": true,
    "editor.guides.bracketPairsHorizontal": false,
    "editor.guides.bracketPairs": true,
    "editor.guides.indentation": false,
    "omnisharp.monoPath": "/Library/Frameworks/Mono.framework/Versions/Current",
    "dotnet.codeLens.enableReferencesCodeLens": true,
    "window.commandCenter": true,
    "workbench.layoutControl.enabled": true,
    "workbench.activityBar.location": "hidden",
    "typescript.validate.enable": true,
    "javascript.validate.enable": true,
}